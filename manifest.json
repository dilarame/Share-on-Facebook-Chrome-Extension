{
  "name": "My 1st Extension",
  "description": "Shows a menu item",
  "version": "1.0",
  "permissions": ["contextMenus","activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
    "matches": ["*://www.google.com/*"],
    "js": ["index.js"]
    }
  ],
  "manifest_version": 2,
  "icons" : {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png" 
  }
  
}


